{
  "name": "Heartlist",
  "version": "0.0.1",
  "description": "Easily heart songs on Spotify",
  "license": "MIT",
  "repository": "https://github.com/acousens/heartlist",
  "devDependencies": {
    "electron": "^8.3.0",
    "electron-builder": "^20.15.1"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "qs": "^6.9.4",
    "dotenv": "^8.2.0"
  },
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "postinstall": "install-app-deps",
    "pack": "build --dir",
    "dist": "build"
  },
  "build": {
    "appId": "com.foo.heartlist",
    "directories": {
      "buildResources": "./build"
    },
    "mac": {
      "extendInfo": {
        "LSUIElement": 1
      }
    }
  }
}
